{
  "name": "currency-viewer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.1.1",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.1.1",
    "react-scripts": "1.0.17",
    "redux-thunk-actions": "^1.1.6"
  },
  "scripts": {
    "build:css": "lessc src/styles/app.less src/styles/app.css",
